{
  "name": "js-boiler-plate",
  "version": "0.0.1",
  "description": "",
  "main": "lib/index.js",
  "jsnext:main": "index.js",
  "scripts": {
    "lint": "xo",
    "test": "ava",
    "build": "rollup index.js --format cjs --output ./dist/index.js",
    "prepublish": "xo && ava && babel src --out-dir lib"
  },
  "author": "billiam",
  "license": "ISC",
  "dependencies": {
  },
  "devDependencies": {
    "ava": "^0.21.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "rollup": "^0.45.2",
    "xo": "^0.18.2"
  },
  "xo": {
    "semicolon": false,
    "space": 2
  },
  "babel": {
    "presets": [
      "@ava/stage-4"
    ]
  },
  "ava": {
    "files": [
      "tests/*.js"
    ],
    "source": [
      "src/*.js",
      "!dist/**/*"
    ],
    "require": [
      "babel-register"
    ]
  }
}
